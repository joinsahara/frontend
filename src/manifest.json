{
  "name": "Sahara",
  "version": "1.0",
  "description": "Your Shopping Assistant",
  "manifest_version": 2,
  "permissions": [
    "activeTab",
    "webNavigation"
  ],
  "background": {
    "scripts": [
        "background.js",
        "runtime.js"
    ]
  },
  "browser_action": {
    "default_popup": "index.html",
    "default_icon": "logo.png"
  }
}
